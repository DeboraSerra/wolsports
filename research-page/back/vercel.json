{
  "version": 2,
  "builds": [
    {
      "source": "./src/index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "source": "./src/(.*)",
      "destination": "./src/index.js"
    }
  ],
  "rewrites": [
    {
      "source": "./src/(.*)",
      "destination": "./src/index.js"
    }
  ]
}
